{
  "name": "file-index-viewer",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
    "pm2:setup": "mkdir -p logs",
    "pm2:start": "npm run pm2:setup && pm2 start ecosystem.config.js",
    "pm2:start:prod": "npm run pm2:setup && pm2 start ecosystem.config.js --env production",
    "pm2:stop": "pm2 stop ecosystem.config.js",
    "pm2:restart": "pm2 restart ecosystem.config.js",
    "pm2:reload": "pm2 reload ecosystem.config.js",
    "pm2:delete": "pm2 delete ecosystem.config.js",
    "pm2:logs": "pm2 logs",
    "pm2:logs:backend": "pm2 logs file-index-viewer-backend",
    "pm2:logs:frontend": "pm2 logs file-index-viewer-frontend",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.6",
    "typescript": "5.9.2",
    "@types/react": "^19",
    "@types/react-dom": "^19"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.2.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "patchedDependencies": {
    "pdf-parse@1.1.1": "patches/pdf-parse@1.1.1.patch"
  }
}